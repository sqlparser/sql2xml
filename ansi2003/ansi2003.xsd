						<xs:element ref="kw_scroll"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="cursor_holdability">
	<xs:complexType>
		<xs:choice>
