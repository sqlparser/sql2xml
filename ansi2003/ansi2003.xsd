	</xs:complexType>
</xs:element>
	
<xs:element name="merge_operation_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="merge_when_clause" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="merge_when_clause">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="merge_when_matched_clause"/>
			<xs:element ref="merge_when_not_matched_clause"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="kw_matched">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="matched"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="merge_when_matched_clause">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_when"/>
			<xs:element ref="kw_matched"/>
			<xs:element ref="kw_then"/>
			<xs:element ref="merge_update_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="merge_when_not_matched_clause">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_when"/>
			<xs:element ref="kw_not"/>
			<xs:element ref="kw_matched"/>
			<xs:element ref="kw_then"/>
			<xs:element ref="merge_insert_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
	
<xs:element name="merge_update_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_update"/>
			<xs:element ref="kw_set"/>
			<xs:element ref="set_clause_list"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="merge_insert_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_insert"/>
			<xs:element ref="insert_column_list" minOccurs="0"/>
			<xs:element ref="override_clause" minOccurs="0"/>
			<xs:element ref="kw_values"/>
			<xs:element ref="merge_insert_value_list"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="merge_insert_value_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="merge_insert_value_element" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="merge_insert_value_element">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="value_expression"/>
			<xs:element ref="contextually_typed_value_specification"/>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="update_statement_positioned">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_update"/>
			<xs:element ref="target_table"/>
			<xs:element ref="as_correlation_name" minOccurs="0"/>
			<xs:element ref="kw_set"/>
			<xs:element ref="set_clause_list"/>
			<xs:element ref="kw_where"/>
			<xs:element ref="kw_current"/>
			<xs:element ref="kw_of"/>
			<xs:element ref="cursor_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="update_statement_searched">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_update"/>
