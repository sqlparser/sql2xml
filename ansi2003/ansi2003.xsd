					<xs:sequence>
						<xs:element ref="kw_breadth"/>
						<xs:element ref="kw_first"/>
						<xs:element ref="kw_by"/>
						<xs:element ref="column_name_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>


