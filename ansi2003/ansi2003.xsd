					<xs:sequence>
						<xs:sequence>
							<xs:element ref="introducer"/>
							<xs:element ref="character_set_specification"/>
						</xs:sequence>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ampersand"/>
			<xs:element name="quoted_unicode_representation" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="quote"/>
						<xs:element ref="unicode_representation" minOccurs="0"/>
						<xs:element ref="quote"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="unicode_escape_specifier" minOccurs="0"/>
