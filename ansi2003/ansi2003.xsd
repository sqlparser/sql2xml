		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="alter_sequence_generator_restart_option">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_restart"/>
			<xs:element ref="kw_with"/>
			<xs:element ref="sequence_generator_restart_value"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="sequence_generator_restart_value">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="signed_numeric_literal"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="drop_sequence_generator_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_drop"/>
			<xs:element ref="kw_sequence"/>
			<xs:element ref="sequence_generator_name"/>
			<xs:element ref="drop_behavior"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<!-- Access control  -->	
<xs:element name="grant_statement">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="grant_privilege_statement"/>
			<xs:element ref="grant_role_statement"/>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="kw_grant">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="grant"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

	<xs:element name="kw_granted">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="granted"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="kw_hierarchy">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="hierarchy"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="grant_privilege_statement">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_grant"/>
			<xs:element ref="privileges"/>
			<xs:element ref="kw_to"/>
			<xs:element ref="grantee" maxOccurs="unbounded"/>
			<xs:element name="with_hierarchy_option" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_with"/>
						<xs:element ref="kw_hierarchy"/>
						<xs:element ref="kw_option"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="with_grant_option" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_with"/>
						<xs:element ref="kw_grant"/>
						<xs:element ref="kw_option"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="granted_by_grantor" minOccurs="0"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="granted_by_grantor">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_granted"/>
			<xs:element ref="kw_by"/>
			<xs:element ref="grantor"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
	
<xs:element name="privileges">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="object_privileges"/>
			<xs:element ref="kw_on"/>
			<xs:element ref="object_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="object_name">
	<xs:complexType>
		<xs:choice>
			<xs:element name="table_name_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_table" minOccurs="0"/>
						<xs:element ref="table_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="domain_name_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_domain"/>
						<xs:element ref="domain_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="collation_name_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_collation"/>
						<xs:element ref="collation_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="character_set_name_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_character"/>
						<xs:element ref="kw_set"/>
						<xs:element ref="character_set_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="translation_name_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_translation"/>
						<xs:element ref="transliteration_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="type_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_type"/>
						<xs:element ref="schema_resolved_user_defined_type_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="sequence_generator_name_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_sequence"/>
						<xs:element ref="sequence_generator_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="specific_routine_designator"></xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
	<xs:element name="kw_privileges">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="privileges"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>

<xs:element name="object_privileges">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="all_privileges">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_all"/>
						<xs:element ref="kw_privileges"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="action_list">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="action" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="action">
	<xs:complexType>
		<xs:choice>
			<xs:element	ref="kw_select"/>
			<xs:element name="select_privilege_column_list">
				<xs:complexType>
					<xs:sequence>
						<xs:element	ref="kw_select"/>
						<xs:element ref="privelege_column_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="select_privilege_method_list">
				<xs:complexType>
					<xs:sequence>
						<xs:element	ref="kw_select"/>
						<xs:element ref="privelege_method_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element	ref="kw_delete"/>
			<xs:element name="insert_privilege_column_list">
				<xs:complexType>
					<xs:sequence>
						<xs:element	ref="kw_insert"/>
						<xs:element ref="privelege_column_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="update_privilege_column_list">
				<xs:complexType>
					<xs:sequence>
						<xs:element	ref="kw_update"/>
						<xs:element ref="privelege_column_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="reference_privilege_column_list">
				<xs:complexType>
					<xs:sequence>
						<xs:element	ref="kw_references"/>
						<xs:element ref="privelege_column_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element	ref="kw_usage"/>
			<xs:element	ref="kw_trigger"/>
			<xs:element	ref="kw_under"/>
			<xs:element	ref="kw_execute"/>
		</xs:choice>
	</xs:complexType>
</xs:element>	

<xs:element name="kw_usage">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="usage"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="kw_execute">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="execute"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="privelege_method_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="specific_routine_designator" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="privelege_column_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="column_name_list"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="grantee">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_public"/>
			<xs:element ref="authorization_identifier"/>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="kw_public">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="public"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="grantor">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_current_user"/>
			<xs:element ref="kw_current_role"/>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="role_definition">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_create"/>
			<xs:element ref="kw_role"/>
			<xs:element ref="role_name"/>
			<xs:element name="with_admin_grantor">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_with"/>
						<xs:element ref="kw_admin"/>
						<xs:element ref="grantor"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="kw_admin">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="admin"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="grant_role_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_grant"/>
			<xs:element ref="role_granted" maxOccurs="unbounded"/>
			<xs:element ref="kw_to"/>
			<xs:element ref="grantee" maxOccurs="unbounded"/>
			<xs:element name="with_admin_option" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_with"/>
						<xs:element ref="kw_admin"/>
						<xs:element ref="kw_option"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="granted_by_grantor" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="role_granted">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="role_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	

<xs:element name="kw_role">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="role"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="drop_role_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_drop"/>
			<xs:element ref="kw_role"/>
			<xs:element ref="role_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="revoke_statement">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="revoke_privilege_statement"/>
			<xs:element ref="revoke_role_statement"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_revoke">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="revoke"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="revoke_privilege_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_revoke"/>
			<xs:element ref="revoke_option_extension" minOccurs="0"/>
			<xs:element ref="privileges"/>
			<xs:element ref="kw_from"/>
			<xs:element ref="grantee" maxOccurs="unbounded"/>
			<xs:element ref="granted_by_grantor" minOccurs="0"/>
			<xs:element ref="drop_behavior"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="revoke_option_extension">
	<xs:complexType>
		<xs:choice>
			<xs:element name="grant_option_for">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_grant"/>
						<xs:element ref="kw_option"/>
						<xs:element ref="kw_for"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="hierarchy_option_for">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_hierarchy"/>
						<xs:element ref="kw_option"/>
						<xs:element ref="kw_for"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="revoke_role_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_revoke"/>
			<xs:element name="admin_option_for" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_admin"/>
						<xs:element ref="kw_option"/>
						<xs:element ref="kw_for"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="role_revoked" maxOccurs="unbounded"/>
			<xs:element ref="kw_from"/>
			<xs:element ref="grantee" maxOccurs="unbounded"/>
			<xs:element ref="granted_by_grantor" minOccurs="0"/>
			<xs:element ref="drop_behavior"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="role_revoked">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="role_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	

<!-- SQL-client modules  -->

<xs:element name="sql_client_module_definition">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="module_name_clause"/>
			<xs:element ref="language_clause"/>
			<xs:element ref="module_authorization_clause"/>
			<xs:element ref="module_path_specification" minOccurs="0"/>
			<xs:element ref="module_transform_group_specification" minOccurs="0"/>
			<xs:element ref="module_collations" minOccurs="0"/>
			<xs:element name="temporary_table_declaration_list" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="temporary_table_declaration" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="module_contents" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="schema_schema_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_schema"/>
			<xs:element ref="schema_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="module_authorization_clause">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="schema_schema_name"/>
			<xs:element name="module_authorization_identifier_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_authorization"/>
						<xs:element ref="module_authorization_identifier"/>
						<xs:element ref="for_static_only_and_dynamic" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="schema_name_module_authorization_identifier_clause">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_schema"/>
						<xs:element ref="schema_name"/>
						<xs:element ref="kw_authorization"/>
						<xs:element ref="module_authorization_identifier"/>
						<xs:element ref="for_static_only_and_dynamic" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>	
	
<xs:element name="for_static_only_and_dynamic">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_for"/>
			<xs:element ref="kw_static"/>
			<xs:element name="only_and_dynamic">
				<xs:complexType>
					<xs:sequence>
						<xs:choice>
							<xs:element ref="kw_only"/>
							<xs:element name="and_dynamic">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="kw_and"/>
										<xs:element ref="kw_dynamic"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="module_authorization_identifier">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="authorization_identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="module_path_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="path_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="module_transform_group_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="transform_group_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="module_collations">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="module_collation_specification" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="module_collation_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_collation"/>
			<xs:element ref="collation_name"/>
			<xs:element name="for_character_set_specification_list" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_for"/>
						<xs:element ref="character_set_specification_list"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="kw_collation"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
	
<xs:element name="character_set_specification_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="character_set_specification" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="module_contents">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="declare_cursor"/>
			<xs:element ref="dynamic_declare_cursor"/>
			<xs:element ref="externally_invoked_procedure"/>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="module_name_clause">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_module"/>
			<xs:element ref="sql_client_module_name" minOccurs="0"/>
			<xs:element ref="module_character_set_specification" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	

<xs:element name="kw_names">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="names"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="kw_are">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="are"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="module_character_set_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_names"/>
			<xs:element ref="kw_are"/>
			<xs:element ref="character_set_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="externally_invoked_procedure">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_procedure"/>
			<xs:element ref="procedure_name"/>
			<xs:element ref="host_parameter_declaration_list"/>
			<xs:element ref="semicolon"/>
			<xs:element ref="sql_procedure_statement"/>
			<xs:element ref="semicolon"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="host_parameter_declaration_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="host_parameter_declaration" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="host_parameter_declaration">
	<xs:complexType>
		<xs:choice>
			<xs:element name="host_parameter_name_data_type">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="host_parameter_name"/>
						<xs:element ref="host_parameter_data_type"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="status_parameter"/>
		</xs:choice>
	</xs:complexType>
</xs:element>	
	
<xs:element name="host_parameter_data_type">
	<xs:complexType>
