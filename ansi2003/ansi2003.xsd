		<xs:sequence>
			<xs:element ref="identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="schema_qualified_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="schema_name" minOccurs="0"/>
			<xs:element ref="qualified_identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="local_or_schema_qualified_name">
	<xs:complexType>
		<xs:sequence>
