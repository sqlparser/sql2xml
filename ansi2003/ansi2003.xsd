			<xs:element ref="kw_alter"/>
			<xs:element ref="kw_sequence"/>
			<xs:element ref="sequence_generator_name"/>
			<xs:element ref="alter_sequence_generator_options"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="alter_sequence_generator_options">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="alter_sequence_generator_option" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>		

<xs:element name="alter_sequence_generator_option">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="alter_sequence_generator_restart_option"/>
			<xs:element ref="basic_sequence_generator_option"/>
		</xs:choice>
	</xs:complexType>
</xs:element>	
	
<xs:element name="kw_restart">
	<xs:simpleType>
		<xs:restriction base="xs:string">
