			<xs:element ref="kw_into"/>
			<xs:element ref="target_table"/>
			<xs:element ref="as_merge_correlation_name" minOccurs="0"/>
			<xs:element ref="kw_using"/>
			<xs:element ref="table_reference"/>
			<xs:element ref="kw_on"/>
			<xs:element ref="search_condition"/>
			<xs:element ref="merge_operation_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="as_merge_correlation_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element	ref="kw_as" minOccurs="0"/>
