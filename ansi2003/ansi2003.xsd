<xs:element name="query_expression">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="with_clause" minOccurs="0"/>
			<xs:element ref="query_expression_body"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="table_expression">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="from_clause"/>
			<xs:element ref="where_clause" minOccurs="0"/>
			<xs:element ref="group_by_clause" minOccurs="0"/>
			<xs:element ref="having_clause" minOccurs="0"/>
			<xs:element ref="window_clause" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_select">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="select"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
	
<xs:element name="query_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_select"/>
			<xs:element ref="set_quantifier" minOccurs="0"/>
			<xs:element ref="select_list"/>
			<xs:element ref="table_expression"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="select_list">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="asterisk"/>
			<xs:element ref="select_sublist" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="select_sublist">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="derived_column"/>
			<xs:element ref="qualified_asterisk"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="qualified_asterisk">
	<xs:complexType>
		<xs:choice>
			<xs:element name="asterisked_identifier_chain_asterisk">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="asterisked_identifier_chain"/>
						<xs:element ref="asterisk"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="all_fields_reference"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="asterisked_identifier_chain">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="asterisked_identifier" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="asterisked_identifier">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<xs:element name="identifier">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="actual_identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="actual_identifier">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="regular_identifier"/>
			<xs:element ref="delimited_identifier"/>
			<xs:element ref="unicode_delimited_identifier"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="sql_language_identifier" type="xs:string"/>


<xs:element name="sql_language_identifier_start">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="simple_latin_letter"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<xs:element name="sql_language_identifier_part">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="simple_latin_letter"/>
			<xs:element ref="digit"/>
			<xs:element ref="underscore"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="authorization_identifier">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="role_name"/>
			<xs:element ref="user_identifier"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="table_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="local_or_schema_qualified_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="domain_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="schema_qualified_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="schema_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="catalog_name" minOccurs="0"/>
			<xs:element ref="unqualified_schema_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="unqualified_schema_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="catalog_name">
