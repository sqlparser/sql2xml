			<xs:element ref="cursor_properties"/>
			<xs:element ref="kw_for"/>
			<xs:element ref="cursor_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>		
	
<xs:element name="kw_declare">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="declare"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>	
	
<xs:element name="cursor_properties">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="cursor_sensitivity" minOccurs="0"/>
			<xs:element ref="cursor_scrollability" minOccurs="0"/>
