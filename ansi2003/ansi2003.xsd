			<xs:element ref="unicode_character_escape_value"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="unicode_4_digit_escape_value">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="unicode_escape_character"/>
			<xs:element ref="hexit" maxOccurs="4" minOccurs="4"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<xs:element name="unicode_6_digit_escape_value">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="unicode_escape_character"/>
			<xs:element ref="plus_sign"/>
			<xs:element ref="hexit" maxOccurs="6" minOccurs="6"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="hexit" >
	<xs:complexType>
		<xs:choice>
			<xs:element ref="digit"/>
			<xs:element name="a_to_f">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[a-f][A-F]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="unicode_character_escape_value">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="unicode_escape_character" maxOccurs="2" minOccurs="2"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


