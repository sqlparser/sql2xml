				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>


<xs:element name="kw_left">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="left"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="kw_right">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="kw_full">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="full"/>
