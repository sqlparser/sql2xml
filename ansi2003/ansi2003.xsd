		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="unicode_escape_character" type="xs:string"/>
	



<xs:element name="unicode_identifier_part">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="delimited_identifier_part"/>
			<xs:element ref="unicode_escape_value"/>
		</xs:choice>
	</xs:complexType>
</xs:element>


