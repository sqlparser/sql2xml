	</xs:complexType>
</xs:element>

	<xs:element name="kw_cycle">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="cycle"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
	
<xs:element name="cycle_clause">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_cycle"/>
