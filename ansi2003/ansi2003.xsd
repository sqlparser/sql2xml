				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="year_month_literal" >
	<xs:complexType>
		<xs:choice>
			<xs:element name="years_value_or_minus_months_value">
