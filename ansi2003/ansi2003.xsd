						<xs:element name="minus_months_value" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="minus_sign"/>
									<xs:element ref="months_value"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="months_value"/>
		</xs:choice>
	</xs:complexType>
</xs:element>
	

<xs:element name="day_time_literal" >
	<xs:complexType>
		<xs:choice>
			<xs:element ref="day_time_interval"/>
