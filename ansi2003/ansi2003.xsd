						<xs:element name="minutes_seconds_value" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="minutes_value"/>
									<xs:element ref="seconds_value" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
