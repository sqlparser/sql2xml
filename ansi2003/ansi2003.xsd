					<xs:sequence>
						<xs:element ref="quote"/>
						<xs:element ref="character_representation" minOccurs="0"/>
						<xs:element ref="quote"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
	
<xs:element name="unicode_character_string_literal" >
