<!-- schema definition and manipulation -->
	
<xs:element name="schema_definition">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_create"/>
			<xs:element ref="kw_schema"/>
			<xs:element ref="schema_name_clause"/>
			<xs:element ref="schema_character_set_or_path" minOccurs="0"/>
			<xs:element ref="schema_element" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_create">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="create"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
	<xs:element name="kw_schema">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="schema"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
	<xs:element name="schema_character_set_or_path">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="schema_character_set_specification"/>
				<xs:element ref="schema_path_specification"/>
				<xs:element name="schema_character_set_with_path">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="schema_character_set_specification"/>
							<xs:element ref="schema_path_specification"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="schema_path_with_character_set">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="schema_path_specification"/>
							<xs:element ref="schema_character_set_specification"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>

<xs:element name="schema_name_clause">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="schema_name"/>
			<xs:element name="authorization_identifier">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_authorization"/>
						<xs:element ref="schema_authorization_identifier"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="schema_name_authorization_identifier">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="schema_name"/>
						<xs:element ref="kw_authorization"/>
						<xs:element ref="schema_authorization_identifier"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="kw_authorization">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="authorization"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="schema_authorization_identifier">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="authorization_identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	
	
<xs:element name="schema_character_set_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_default"/>
			<xs:element ref="kw_character"/>
			<xs:element ref="kw_set"/>
			<xs:element ref="character_set_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>	

<xs:element name="schema_path_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="path_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="schema_element">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="table_element"/>
			<xs:element ref="view_definition"/>
			<xs:element ref="domain_definition"/>
			<xs:element ref="character_set_definition"/>
			<xs:element ref="collation_definition"/>
			<xs:element ref="transliteration_definition"/>
			<xs:element ref="assertion_definition"/>
			<xs:element ref="trigger_definition"/>
			<xs:element ref="user_defined_type_definition"/>
