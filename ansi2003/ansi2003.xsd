						<xs:element ref="kw_as" minOccurs="0"/>
						<xs:element ref="new_transition_variable_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="old_table_as">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_old"/>
						<xs:element ref="kw_table" />
