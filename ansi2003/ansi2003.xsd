<?xml version="1.0" encoding="ISO-8859-1" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.sqlparser.com/xml/ansi2003"
	xmlns="http://www.sqlparser.com/xml/ansi2003"
	elementFormDefault="qualified"
	>
	
<xs:annotation>
  <xs:appinfo>General SQL Parser</xs:appinfo>
  <xs:documentation xml:lang="en">
  This Schema defines SQL ANSI2003 BNF Grammars.
  </xs:documentation>
</xs:annotation>
	
	
<!-- Direct invocation of SQL -->	
<xs:element name="direct_sql_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="directly_executable_statement"/>
			<xs:element ref="semicolon"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="directly_executable_statement">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="direct_sql_data_statement"/>
			<xs:element ref="sql_schema_statement"/>
			<xs:element ref="sql_transaction_statement"/>
			<xs:element ref="sql_connection_statement"/>
			<xs:element ref="sql_session_statement"/>
			<xs:element ref="direct_implementation_defined_statement"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="direct_sql_data_statement">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="delete_statement_searched"/>
			<xs:element ref="direct_select_statement_multiple_rows"/>
			<xs:element ref="insert_statement"/>
			<xs:element ref="update_statement_searched"/>
			<xs:element ref="merge_statement"/>
			<xs:element ref="temporary_table_declaration"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="direct_implementation_defined_statement" type="xs:string"/>	

<xs:element name="direct_select_statement_multiple_rows">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="cursor_specification"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	
