			<xs:element ref="kw_set"/>
			<xs:element ref="set_clause_list"/>
			<xs:element ref="where_search_condition" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="set_clause_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="set_clause" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
<xs:element name="set_clause">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="multiple_column_assignment"/>
			<xs:element name="set_target_equals_update_source">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="set_target"/>
						<xs:element ref="equals_operator"/>
						<xs:element ref="update_source"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>
	
<xs:element name="set_target">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="update_target"/>
