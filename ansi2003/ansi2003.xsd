			<xs:element ref="cursor_holdability" minOccurs="0"/>
			<xs:element ref="cursor_returnability" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>		
	
<xs:element name="cursor_sensitivity">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_sensitive"/>
			<xs:element ref="kw_insensitive"/>
			<xs:element ref="kw_asensitive"/>
		</xs:choice>
	</xs:complexType>
</xs:element>	
	
<xs:element name="kw_sensitive">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="sensitive"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="kw_insensitive">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="insensitive"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="kw_asensitive">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="asensitive"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>	
	
<xs:element name="kw_scroll">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="scroll"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>	
	
<xs:element name="kw_hold">
	<xs:simpleType>
		<xs:restriction base="xs:string">
