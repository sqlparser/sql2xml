			<xs:element ref="kw_covar_samp"/>
			<xs:element ref="kw_corr"/>
			<xs:element ref="kw_regr_slope"/>
			<xs:element ref="kw_regr_intersect"/>
			<xs:element ref="kw_regr_count"/>
			<xs:element ref="kw_regr_r2"/>
			<xs:element ref="kw_regr_avgx"/>
			<xs:element ref="kw_regr_avgy"/>
			<xs:element ref="kw_regr_sxx"/>
			<xs:element ref="kw_regr_sxy"/>
