<xs:element name="time_zone">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_at"/>
			<xs:element ref="time_zone_specifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="time_zone_specifier">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_local"/>
			<xs:element name="time_zone_interval_primary">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_time"/>
						<xs:element ref="kw_zone"/>
						<xs:element ref="interval_primary"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="datetime_value_function">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="current_date_value_function"/>
			<xs:element ref="current_time_value_function"/>
			<xs:element ref="current_timestamp_value_function"/>
			<xs:element ref="current_local_time_value_function"/>
			<xs:element ref="current_local_timestamp_value_function"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="current_date_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_current_date"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<xs:element name="kw_current_time">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="current_time"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="current_time_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_current_time"/>
			<xs:element ref="time_precision" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="kw_localtime">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="localtime"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
	
<xs:element name="current_local_time_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_localtime"/>
			<xs:element ref="time_precision" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="kw_current_timestamp">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="current_timestamp"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="current_timestamp_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_current_timestamp"/>
			<xs:element ref="timestamp_precision" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="kw_localtimestamp">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="localtimestamp"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="current_local_timestamp_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_localtimestamp"/>
			<xs:element ref="timestamp_precision" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="interval_value_expression">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="interval_term"/>
			<xs:element name="interval_value_expression_plus_interval_term">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="interval_value_expression_1"/>
						<xs:element ref="interval_term_1"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="interval_value_expression_minus_interval_term">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="interval_value_expression_1"/>
						<xs:element ref="interval_term_1"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="datetime_value_expression_minus_datetime_term">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="datetime_value_expression"/>
						<xs:element ref="datetime_term"/>
						<xs:element ref="interval_qualifier"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="interval_term">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="interval_factor"/>
			<xs:element name="interval_term_asterisk">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="interval_term_2"/>
						<xs:element ref="factor"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="interval_term_solidus">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="interval_term_2"/>
						<xs:element ref="factor"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="term_asterisk_interval_factor">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="term"/>
						<xs:element ref="interval_factor"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>


<xs:element name="interval_factor">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="sign" minOccurs="0"/>
			<xs:element ref="interval_primary"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="sign">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="plus_sign"/>
			<xs:element ref="minus_sign"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="interval_primary">
	<xs:complexType>
		<xs:choice>
			<xs:element name="value_expression_primary_interval_qualifier">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="value_expression_primary"/>
						<xs:element ref="interval_qualifier" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="interval_value_function"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="interval_value_expression_1">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="interval_value_expression"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="interval_term_1">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="interval_term"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="interval_term_2">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="interval_term"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="interval_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="interval_absolute_value_function"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="interval_absolute_value_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_abs"/>
			<xs:element ref="interval_value_expression"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<xs:element name="kw_to">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="to"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<!-- additional common elements -->
	
<xs:element name="interval_qualifier">
	<xs:complexType>
		<xs:choice>
			<xs:element name="start_to_end_field">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="start_field"/>
						<xs:element ref="kw_to"/>
						<xs:element ref="end_field"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="single_datetime_field"/>
		</xs:choice>
	</xs:complexType>
</xs:element>


<xs:element name="start_field">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="non_second_primary_datetime_field"/>
			<xs:element ref="interval_leading_field_precision" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="end_field">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="non_second_primary_datetime_field"/>
			<xs:element name="second_interval_fractional_seconds_precision">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_second"/>
						<xs:element ref="interval_fractional_seconds_precision" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="single_datetime_field">
	<xs:complexType>
		<xs:choice>
			<xs:element name="single_datetime_field_1">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="non_second_primary_datetime_field"/>
						<xs:element ref="interval_leading_field_precision" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="single_datetime_field_2">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_second"/>
						<xs:element name="interval_leading_field_precision_comma" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="interval_leading_field_precision"/>
									<xs:element ref="interval_fractional_seconds_precision" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="primary_datetime_field">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="non_second_primary_datetime_field"/>
			<xs:element ref="kw_second"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="non_second_primary_datetime_field">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_year"/>
			<xs:element ref="kw_month"/>
			<xs:element ref="kw_day"/>
			<xs:element ref="kw_hour"/>
			<xs:element ref="kw_minute"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="interval_fractional_seconds_precision">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="unsigned_integer"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="interval_leading_field_precision">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="unsigned_integer"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_collate">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="collate"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="collate_clause">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_collate"/>
			<xs:element ref="collation_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_language">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="language"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="language_clause">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_language"/>
			<xs:element ref="language_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="language_name">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_ada"/>
			<xs:element ref="kw_c"/>
			<xs:element ref="kw_cobol"/>
			<xs:element ref="kw_fortran"/>
			<xs:element ref="kw_m"/>
			<xs:element ref="kw_mumps"/>
			<xs:element ref="kw_pascal"/>
			<xs:element ref="kw_pli"/>
			<xs:element ref="kw_sql"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_path">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="path"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="path_specification">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_path"/>
			<xs:element ref="schema_name_list"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="schema_name_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="schema_name" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="routine_invocation">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="routine_name"/>
			<xs:element ref="sql_argument_list"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="routine_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="schema_name" minOccurs="0"/>
			<xs:element ref="qualified_identifier"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="sql_argument_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="sql_argument" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<xs:element name="sql_argument">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="value_expression"/>
			<xs:element ref="generalized_expression"/>
			<xs:element ref="target_specification"/>
		</xs:choice>
	</xs:complexType>
</xs:element>


<xs:element name="generalized_expression">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="value_expression"/>
			<xs:element ref="kw_as"/>
			<xs:element ref="path_resolved_user_defined_type_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="character_set_specification">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="standard_character_set_name"/>
			<xs:element ref="implementation_defined_character_set_name"/>
			<xs:element ref="user_defined_character_set_name"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="standard_character_set_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="character_set_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="implementation_defined_character_set_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="character_set_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="user_defined_character_set_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="character_set_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_specific">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="specific"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="specific_routine_designator">
	<xs:complexType>
		<xs:choice>
			<xs:element name="routine_type_specific_name">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_specific"/>
						<xs:element ref="routine_type"/>
						<xs:element ref="specific_name"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="routine_type_member_name">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="routine_type"/>
						<xs:element ref="member_name"/>
						<xs:element name="for_schema_resolved_user_defined_type_name"  minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="kw_for"/>
									<xs:element ref="schema_resolved_user_defined_type_name"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="routine_type">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_routine"/>
			<xs:element ref="kw_function"/>
			<xs:element ref="kw_procedure"/>
			<xs:element name="instance_static_constructor_method">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="instance_static_constructor" minOccurs="0">
							<xs:complexType>
								<xs:choice>
									<xs:element ref="kw_instance"/>
									<xs:element ref="kw_static"/>
									<xs:element ref="kw_constructor"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element ref="kw_method"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>


<xs:element name="member_name">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="member_name_alternatives"/>
			<xs:element ref="data_type_list" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="member_name_alternatives">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="schema_qualified_routine_name"/>
			<xs:element ref="method_name"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="data_type_list">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="data_type" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="kw_constraint">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="constraint"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="constraint_name_definition">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_constraint"/>
			<xs:element ref="constraint_name"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="constraint_characteristics">
	<xs:complexType>
		<xs:choice>
			<xs:element name="constraint_check_time_deferrable">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="constraint_check_time"/>
						<xs:element name="kw_not_deferrable" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="kw_not" minOccurs="0"/>
									<xs:element ref="kw_deferrable"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="deferrable_constraint_check_time">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="kw_not_deferrable" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="kw_not" minOccurs="0"/>
									<xs:element ref="kw_deferrable"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element ref="constraint_check_time" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

	<xs:element name="kw_initially">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="initially"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	
<xs:element name="kw_deferred">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="deferred"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>

<xs:element name="kw_immediate">
	<xs:simpleType>
		<xs:restriction base="xs:string">
			<xs:enumeration value="immedidate"/>
		</xs:restriction>
	</xs:simpleType>
</xs:element>
	
<xs:element name="constraint_check_time">
	<xs:complexType>
		<xs:choice>
			<xs:element ref="kw_initially_deferred"/>
			<xs:element ref="kw_initially_immediate"/>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="kw_initially_deferred">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_initially"/>
			<xs:element ref="kw_deferred"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>

	
<xs:element name="kw_initially_immediate">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_initially"/>
			<xs:element ref="kw_immediate"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	

<xs:element name="aggregate_function">
	<xs:complexType>
		<xs:choice>
			<xs:element name="aggregate_count">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="kw_count"/>
						<xs:element ref="asterisk"/>
						<xs:element ref="filter_clause" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="aggregate_function_general_set">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="general_set_function"/>
						<xs:element ref="filter_clause" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="aggregate_function_binary_set">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="binary_set_function"/>
						<xs:element ref="filter_clause" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="aggregate_function_ordered_set">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ordered_set_function"/>
						<xs:element ref="filter_clause" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
</xs:element>

<xs:element name="general_set_function">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="set_function_type"/>
			<xs:element ref="set_quantifier" minOccurs="0"/>
			<xs:element ref="value_expression"/>
		</xs:sequence>
