		</xs:choice>
	</xs:complexType>
</xs:element>	
	
<xs:element name="hold_locator_statement">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="kw_hold"/>
			<xs:element ref="kw_locator"/>
			<xs:element ref="locator_reference" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
	
