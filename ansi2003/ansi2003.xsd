					<xs:sequence>
						<xs:element ref="minutes_value"/>
						<xs:element ref="seconds_value" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="seconds_value"/>
		</xs:choice>
	</xs:complexType>
